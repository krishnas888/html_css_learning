
<!--Define an HTML from for user input form-->
<form></form>

<!--Define an input control-->
<input>

<!--Define a multiline input control (text area) textarea -->
<textarea></textarea>

<!--Define a clickable button-->
<button>

<!--Define a drop-down list <select>-->

<select></select>

<!--Defines a froup of related options in a drop-down list optgroup-->
<optgroup></optgroup>

<!--Define an option in a drop-down list-->
<option></option>

<!--Define a label for an <input> element-->
<label></label>

<!--Groups related element in a form-->
<fieldset></fieldset>

<!--Defines a caprtion for a <fieldset> element legend-->
<legend></legend> 

<!--Specifies a list of pre-defined options for input controls detalist-->
<datalist></detalist>

<!--Define the result of a calculation output-->
<output></output>